{
	"name": "BlueSpiceUserMergeConnector",
	"license-name": "GPL-3.0-only",
	"version": "3.1.0-alpha",
	"author": [
		"Patric Wirth"
	],
	"url": "https://en.wiki.bluespice.com/wiki/Reference:BlueSpiceUserMergeConnector",
	"descriptionmsg": "bs-usermergeconnector-extension-description",
	"type": "bluespice",
	"callback": "\\BlueSpice\\UserMergeConnector\\Extension::onRegistration",
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceUserMergeConnector": {
					"className": "\\BlueSpice\\UserMergeConnector\\Extension",
					"extPath": "/BlueSpiceUserMergeConnector"
				}
			},
			"PermissionRegistry": {
				"usermerge": {
					"type": "global",
					"roles": [ "admin", "accountmanager" ]
				}
			}
		}
	},
	"AutoloadNamespaces": {
		"BlueSpice\\UserMergeConnector\\": "src/"
	},
	"MessagesDirs": {
		"BlueSpiceUserMergeConnector": [
			"i18n"
		]
	},
	"GroupPermissions": {
		"bureaucrat": {
			"usermerge": true
		},
		"sysop": {
			"usermerge": true
		}
	},
	"Hooks": {
		"UserMergeAccountFields": "\\BlueSpice\\UserMergeConnector\\Extension::UserMergeAccountFields",
		"MergeAccountFromTo": [
			"\\BlueSpice\\UserMergeConnector\\Extension::onMergeAccountFromToManageReviewTemplates",
			"\\BlueSpice\\UserMergeConnector\\Extension::onMergeAccountFromToManageBSSocial",
			"\\BlueSpice\\UserMergeConnector\\Extension::onMergeAccountFromToManagePageAssignments"
		]
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
